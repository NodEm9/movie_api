{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node-server",
      "config": {
        "zeroConfig": true
      }
    },
    {
      "src": "public/**/*",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "/": "index.html",
      "/movies": "/movies",
      "users": "/users",
      "/documentation.html": "documentation.html"
    }
  ]
}